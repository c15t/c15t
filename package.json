{
	"name": "c15t",
	"private": true,
	"scripts": {
		"build:apps": "turbo build --filter=./apps/*",
		"build:libs": "turbo build --filter=./packages/*",
		"build": "turbo build --filter=!./apps/examples/*",
		"check-types": "turbo check-types",
		"dev:docs": "turbo dev --filter=./apps/docs/*",
		"dev:libs": "turbo dev --filter=./packages/*",
		"dev": "turbo dev --filter=!./apps/examples/*",
		"fmt": "turbo fmt",
		"knip": "knip",
		"lint:fix": "turbo lint -- --write",
		"lint": "turbo lint",
		"postinstall": "lefthook install",
		"test": "turbo test --filter=!./apps/examples/*",
		"view-report": "turbo run view-report",
		"packages:bump": "pnpm dlx bumpp -r",
		"packages:publish": "pnpm build:libs && pnpm --filter='./packages/*' publish -r --access=public",
		"packages:changelog:github": "pnpm dlx changelogithub --draft"
	},
	"resolutions": {
		"@libsql/kysely-libsql": "^0.4.1"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@rsbuild/core": "1.3.5",
		"@rsbuild/plugin-react": "^1.2.0",
		"@rslib/core": "^0.6.2",
		"@types/node": "22.14.1",
		"@types/react": "^19.1.1",
		"@types/react-dom": "^19.1.2",
		"@vitejs/plugin-react": "^4.3.4",
		"@vitest/browser": "^3.1.1",
		"@vitest/coverage-istanbul": "^3.1.1",
		"bumpp": "^9.3.0",
		"changelogithub": "^0.13.11",
		"knip": "^5.50.4",
		"lefthook": "^1.11.10",
		"playwright": "^1.51.1",
		"turbo": "^2.5.0",
		"typescript": "5.8.3",
		"vitest": "^3.1.1",
		"vitest-browser-react": "^0.1.1"
	},
	"packageManager": "pnpm@10.8.0",
	"engines": {
		"node": ">=22"
	}
}
