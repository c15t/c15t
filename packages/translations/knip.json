{
	"$schema": "https://unpkg.com/knip@latest/schema.json",
	"entry": ["./src/index.ts", "./src/adapters/next.ts"],
	"ignoreBinaries": ["biome", "rslib", "tsc", "vitest"],
	"ignore": [
		"**/*.spec.{js,ts,tsx}",
		"**/*.test.{js,ts,tsx}",
		"**/__tests__/**",
		"rslib.config.ts"
	],
	"ignoreDependencies": ["@c15t/typescript-config", "c15t", "zustand"],
	"rules": {
		"exports": "warn",
		"types": "warn",
		"dependencies": "error",
		"files": "warn"
	},
	"ignoreExportsUsedInFile": true
}
