{
	"name": "@c15t/backend",
	"version": "1.5.0-canary-20250722133547",
	"license": "GPL-3.0-only",
	"type": "module",
	"exports": {
		"./v2": {
			"types": "./dist/v2/core.d.ts",
			"import": "./dist/v2/core.js",
			"require": "./dist/v2/core.cjs"
		},
		"./v2/router": {
			"types": "./dist/v2/router.d.ts",
			"import": "./dist/v2/router.js",
			"require": "./dist/v2/router.cjs"
		},
		"./v2/schema": {
			"types": "./dist/v2/schema/index.d.ts",
			"import": "./dist/v2/schema/index.js",
			"require": "./dist/v2/schema/index.cjs"
		},
		"./v2/contracts": {
			"types": "./dist/v2/contracts/index.d.ts",
			"import": "./dist/v2/contracts/index.js",
			"require": "./dist/v2/contracts/index.cjs"
		},
		"./v2/pkgs/db-adapters": {
			"types": "./dist/v2/pkgs/db-adapters/index.d.ts",
			"import": "./dist/v2/pkgs/db-adapters/index.js",
			"require": "./dist/v2/pkgs/db-adapters/index.cjs"
		},
		"./v2/pkgs/results": {
			"types": "./dist/v2/pkgs/results/index.d.ts",
			"import": "./dist/v2/pkgs/results/index.js",
			"require": "./dist/v2/pkgs/results/index.cjs"
		}
	},
	"main": "./dist/server.cjs",
	"module": "./dist/server.js",
	"types": "./dist/server.d.ts",
	"scripts": {
		"prebuild": "genversion --esm --semi src/version.ts",
		"build": "pnpm prebuild && rslib build",
		"dev": "pnpm prebuild && rslib build --watch",
		"check-types": "pnpm prebuild && tsc --noEmit",
		"fmt": "pnpm biome format --write . && pnpm biome check --formatter-enabled=false --linter-enabled=false --organize-imports-enabled=true --write",
		"knip": "knip",
		"lint": "pnpm biome lint ./src",
		"start": "node dist/server.cjs",
		"test": "pnpm prebuild && vitest run",
		"test:watch": "pnpm prebuild && vitest"
	},
	"dependencies": {
		"@c15t/translations": "workspace:*",
		"@doubletie/logger": "1.0.4",
		"@opentelemetry/api": "1.8.0",
		"@opentelemetry/resources": "^1.25.1",
		"@opentelemetry/sdk-node": "^0.51.1",
		"@opentelemetry/sdk-trace-base": "^1.25.1",
		"@orpc/contract": "1.2.0",
		"@orpc/openapi": "1.2.0",
		"@orpc/server": "1.2.0",
		"@orpc/zod": "1.2.0",
		"base-x": "^5.0.1",
		"defu": "^6.1.4",
		"drizzle-orm": "^0.41.0",
		"fumadb": "^0.0.9",
		"kysely": "^0.27.6",
		"neverthrow": "^8.2.0",
		"superjson": "^2.2.2",
		"zod": "^3.24.2"
	},
	"devDependencies": {
		"@c15t/typescript-config": "workspace:*",
		"@c15t/vitest-config": "workspace:*",
		"@electric-sql/pglite": "0.2.17",
		"@libsql/kysely-libsql": "^0.4.1",
		"@types/better-sqlite3": "^7.6.13",
		"@types/express": "^5.0.1",
		"@types/node": "20.14.13",
		"@types/pg": "8.11.6",
		"genversion": "^3.2.0",
		"kysely-pglite": "^0.6.1",
		"msw": "^2.7.6",
		"typescript": "^5.8.3",
		"vitest": "^3.1.1"
	},
	"publishConfig": {
		"access": "public"
	}
}
