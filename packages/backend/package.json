{
	"name": "@c15t/backend",
	"version": "1.8.0",
	"description": "Consent policy engine and API for c15t. Powers the cookie banner, consent manager, and preferences centre. Webhooks, audit logs, storage adapters. Self host or use consent.io",
	"keywords": [
		"consent",
		"privacy",
		"gdpr",
		"ccpa",
		"lgpd",
		"backend",
		"self-host",
		"consent-management",
		"user-privacy",
		"compliance",
		"data-protection",
		"cookie-banner",
		"consent-management-platform",
		"cmp",
		"consent-banner"
	],
	"homepage": "https://c15t.com/docs/self-host/v2",
	"repository": {
		"type": "git",
		"url": "https://github.com/c15t/c15t.git",
		"directory": "packages/backend"
	},
	"license": "GPL-3.0-only",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/core.d.ts",
			"import": "./dist/core.js",
			"require": "./dist/core.cjs"
		},
		"./define-config": {
			"types": "./dist/define-config.d.ts",
			"import": "./dist/define-config.js",
			"require": "./dist/define-config.cjs"
		},
		"./router": {
			"types": "./dist/router.d.ts",
			"import": "./dist/router.js",
			"require": "./dist/router.cjs"
		},
		"./contracts": {
			"types": "./dist/contracts/index.d.ts",
			"import": "./dist/contracts.js",
			"require": "./dist/contracts.cjs"
		},
		"./db/schema": {
			"types": "./dist/db/schema/index.d.ts",
			"import": "./dist/schema.js",
			"require": "./dist/schema.cjs"
		},
		"./db/adapters": {
			"types": "./dist/db/adapters/index.d.ts",
			"import": "./dist/adapters.js",
			"require": "./dist/adapters.cjs"
		},
		"./db/adapters/kysely": {
			"types": "./dist/db/adapters/kysely.d.ts",
			"import": "./dist/db/adapters/kysely.js",
			"require": "./dist/db/adapters/kysely.cjs"
		},
		"./db/adapters/drizzle": {
			"types": "./dist/db/adapters/drizzle.d.ts",
			"import": "./dist/db/adapters/drizzle.js",
			"require": "./dist/db/adapters/drizzle.cjs"
		},
		"./db/adapters/prisma": {
			"types": "./dist/db/adapters/prisma.d.ts",
			"import": "./dist/db/adapters/prisma.js",
			"require": "./dist/db/adapters/prisma.cjs"
		},
		"./db/adapters/typeorm": {
			"types": "./dist/db/adapters/typeorm.d.ts",
			"import": "./dist/db/adapters/typeorm.js",
			"require": "./dist/db/adapters/typeorm.cjs"
		},
		"./db/adapters/mongo": {
			"types": "./dist/db/adapters/mongo.d.ts",
			"import": "./dist/db/adapters/mongo.js",
			"require": "./dist/db/adapters/mongo.cjs"
		},
		"./db/migrator": {
			"types": "./dist/db/migrator/index.d.ts",
			"import": "./dist/migrator.js",
			"require": "./dist/migrator.cjs"
		},
		"./types": {
			"types": "./dist/types/index.d.ts",
			"import": "./dist/types.js",
			"require": "./dist/types.cjs"
		}
	},
	"main": "./dist/server.cjs",
	"module": "./dist/server.js",
	"types": "./dist/server.d.ts",
	"scripts": {
		"prebuild": "genversion --esm --semi src/version.ts",
		"build": "pnpm prebuild && rslib build",
		"check-types": "pnpm prebuild && tsc --noEmit",
		"dev": "pnpm prebuild && rslib build --watch",
		"fmt": "pnpm biome format --write . && pnpm biome check --formatter-enabled=false --linter-enabled=false --write",
		"knip": "knip",
		"lint": "pnpm biome lint ./src",
		"start": "node dist/server.cjs",
		"test": "pnpm prebuild && vitest run",
		"test:watch": "pnpm prebuild && vitest"
	},
	"dependencies": {
		"@c15t/logger": "workspace:*",
		"@c15t/translations": "workspace:*",
		"@opentelemetry/api": "1.9.0",
		"@opentelemetry/resources": "^2.0.1",
		"@opentelemetry/sdk-node": "^0.203.0",
		"@opentelemetry/sdk-trace-base": "^2.0.1",
		"@orpc/contract": "1.8.1",
		"@orpc/openapi": "1.8.1",
		"@orpc/otel": "^1.8.1",
		"@orpc/server": "1.8.1",
		"@orpc/zod": "1.8.1",
		"base-x": "^5.0.1",
		"defu": "^6.1.4",
		"drizzle-orm": "^0.44.6",
		"fumadb": "^0.1.1",
		"kysely": "^0.27.6",
		"neverthrow": "^8.2.0",
		"superjson": "^2.2.2",
		"zod": "^4.0.17"
	},
	"devDependencies": {
		"@c15t/typescript-config": "workspace:*",
		"@c15t/vitest-config": "workspace:*",
		"@electric-sql/pglite": "0.2.17",
		"@libsql/kysely-libsql": "^0.4.1",
		"@types/better-sqlite3": "^7.6.13",
		"@types/express": "^5.0.1",
		"@types/node": "20.14.13",
		"@types/pg": "8.11.6",
		"genversion": "^3.2.0",
		"kysely-pglite": "^0.6.1",
		"msw": "^2.7.6",
		"typescript": "^5.8.3"
	},
	"publishConfig": {
		"access": "public"
	}
}
