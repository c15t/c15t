{
	"$schema": "https://unpkg.com/knip@latest/schema.json",
	"entry": [
		"./src/index.ts",
		"./src/core.ts",
		"./src/init.ts",
		"./src/types/index.ts",
		"./src/utils/index.ts",
		"./src/client/index.ts",
		"./src/db/adapters/prisma-adapter/index.ts",
		"./src/db/adapters/drizzle-adapter/index.ts",
		"./src/db/adapters/memory-adapter/index.ts",
		"./src/error/index.ts",
		"./src/error/codes.ts",
		"./src/integrations/index.ts",
		"./src/integrations/next.ts",
		"./src/integrations/react.ts",
		"./src/db/index.ts",
		"./src/plugins/index.ts"
	],
	"project": "**/*.{js,ts,jsx,tsx}",
	"ignoreDependencies": [
		"typescript",
		"@c15t/typescript-config",
		"@better-fetch/fetch",
		"drizzle-orm"
	],
	"ignoreBinaries": ["biome", "rslib"],
	"ignore": ["**/*.spec.{js,ts}", "**/*.test.{js,ts}"],
	"rules": {
		"exports": "warn",
		"types": "warn",
		"dependencies": "error",
		"files": "warn"
	},
	"ignoreExportsUsedInFile": true
}
