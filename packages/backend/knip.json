{
	"$schema": "https://unpkg.com/knip@latest/schema.json",
	"entry": [
		"./src/index.ts",
		"./src/schema/index.ts",
		"./src/integrations/index.ts",
		"./src/integrations/next.ts",
		"./src/pkgs/data-model/fields/index.ts",
		"./src/pkgs/data-model/index.ts",
		"./src/pkgs/data-model/schema/index.ts",
		"./src/pkgs/db-adapters/adapters/drizzle-adapter/index.ts",
		"./src/pkgs/db-adapters/adapters/kysely-adapter/index.ts",
		"./src/pkgs/db-adapters/adapters/memory-adapter/index.ts",
		"./src/pkgs/db-adapters/adapters/prisma-adapter/index.ts",
		"./src/pkgs/db-adapters/index.ts",
		"./src/pkgs/logger/index.ts",
		"./src/pkgs/migrations/index.ts",
		"./src/pkgs/results/index.ts",
		"./src/pkgs/types/index.ts"
	],
	"project": "**/*.{js,ts,jsx,tsx}",
	"ignoreBinaries": ["biome", "rslib"],
	"ignore": ["**/*.spec.{js,ts}", "**/*.test.{js,ts}", "rslib.config.ts"],
	"rules": {
		"exports": "warn",
		"types": "warn",
		"dependencies": "error",
		"files": "warn"
	},
	"ignoreExportsUsedInFile": true
}
