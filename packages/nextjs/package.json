{
	"name": "@c15t/nextjs",
	"version": "1.8.3",
	"description": "Developer-first CMP for Next.js: cookie banner, consent manager, preferences centre. GDPR ready with minimal setup and rich customization.",
	"keywords": [
		"nextjs",
		"consent",
		"privacy",
		"gdpr",
		"ccpa",
		"lgpd",
		"server-side rendering",
		"react",
		"typescript",
		"cookie-banner",
		"consent-management-platform",
		"cmp",
		"consent-banner",
		"user-consent",
		"privacy-compliance",
		"web-privacy"
	],
	"homepage": "https://c15t.com/docs/frameworks/next/quickstart",
	"repository": {
		"type": "git",
		"url": "https://github.com/c15t/c15t.git",
		"directory": "packages/nextjs"
	},
	"license": "GPL-3.0-only",
	"sideEffects": false,
	"type": "module",
	"exports": {
		"./headless": {
			"types": "./dist/headless.d.ts",
			"import": "./dist/headless.js",
			"require": "./dist/headless.cjs"
		},
		"./pages/headless": {
			"types": "./dist/headless.pages.d.ts",
			"import": "./dist/headless.pages.js",
			"require": "./dist/headless.pages.cjs"
		},
		"./pages": {
			"types": "./dist/pages.d.ts",
			"import": "./dist/pages.js",
			"require": "./dist/pages.cjs"
		},
		"./client": {
			"types": "./dist/client.d.ts",
			"import": "./dist/client.js",
			"require": "./dist/client.cjs"
		},
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"prebuild": "genversion --esm --semi src/version.ts",
		"build": "bun prebuild && rslib build",
		"check-types": "bun prebuild && tsc --noEmit",
		"dev": "bun prebuild && rslib build --watch",
		"fmt": "bun biome format --write . && bun biome check --formatter-enabled=false --linter-enabled=false --write",
		"lint": "bun biome lint ./src",
		"test": "bun prebuild && vitest run --passWithNoTests",
		"test:watch": "bun prebuild && vitest --passWithNoTests"
	},
	"dependencies": {
		"@c15t/react": "workspace:*",
		"@c15t/translations": "workspace:*"
	},
	"devDependencies": {
		"@c15t/backend": "workspace:*",
		"@c15t/typescript-config": "workspace:*",
		"@c15t/vitest-config": "workspace:*",
		"genversion": "3.2.0",
		"typescript": "5.9.3"
	},
	"peerDependencies": {
		"next": "^16.0.0 || ^15.0.0 || ^14.0.0 || ^13.0.0",
		"react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0 || ^19.0.0-rc",
		"react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0 || ^19.0.0-rc"
	},
	"publishConfig": {
		"access": "public"
	}
}
