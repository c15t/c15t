{
	"name": "@c15t/cli",
	"version": "0.0.1-rc.13",
	"description": "The CLI for c15t",
	"exports": "./dist/index.mjs",
	"main": "./dist/index.mjs",
	"module": "dist/index.mjs",
	"bin": {
		"cli": "dist/index.mjs"
	},
	"files": ["dist"],
	"scripts": {
		"build": "rslib build",
		"fmt": "pnpm biome format --write . && biome check --formatter-enabled=false --linter-enabled=false --organize-imports-enabled=true --write",
		"lint": "pnpm biome lint ./src",
		"start": "node ./dist/index.mjs",
		"knip": "knip",
		"dev": "rslib dev",
		"test": "vitest"
	},
	"dependencies": {
		"@babel/preset-react": "^7.26.3",
		"@babel/preset-typescript": "^7.26.0",
		"@c15t/backend": "workspace:*",
		"@mrleebo/prisma-ast": "^0.12.1",
		"@types/better-sqlite3": "^7.6.12",
		"@types/prompts": "^2.4.9",
		"better-sqlite3": "^11.8.1",
		"c12": "^3.0.2",
		"chalk": "^5.4.1",
		"commander": "^13.1.0",
		"dotenv": "^16.4.7",
		"fs-extra": "^11.3.0",
		"prompts": "^2.4.2",
		"yocto-spinner": "^0.2.1",
		"zod": "^3.24.2"
	},
	"devDependencies": {
		"@c15t/typescript-config": "workspace:*",
		"@types/fs-extra": "^11.0.4",
		"vitest": "^3.0.8"
	},
	"publishConfig": {
		"access": "public",
		"executableFiles": ["./dist/index.mjs"]
	}
}
