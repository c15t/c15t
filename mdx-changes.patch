+  <CompactCard 
+    href="/docs/storing-consent/offline-mode" 
+    icon={<RiTimerLine size={18} />}
+  >
+    Configure <code>Offline Mode</code>
+  </CompactCard>
+
+  <CompactCard 
+    href="/docs/storing-consent/self-hosting" 
+    icon={<RiServerLine size={18} />}
